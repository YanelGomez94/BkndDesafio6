<div>
    <h3>Chat</h3>
    <input id="chatBox" placeholder="Escribe tu mensaje aqui">
    <p id="messageLogs"></p>
</div>

<div>
    <h1> Mensajes Guardados</h1>
    {{#each messages}}
    <div>
        <p>Usuario: {{this.user}}</p>
        <p>Mensaje: {{this.message}}</p>
    </div>
    {{/each}}
</div>
<script src="//cdn.jsdelivr.net/npm/sweetalert2@11"></script>
<script src="/socket.io/socket.io.js"></script>
<script src="js/index.js"></script>